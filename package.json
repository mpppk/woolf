{
  "name": "woolf",
  "version": "0.0.1",
  "description": "Universal workflow engine on lamool",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf src/**/*.js src/**/*.js.map",
    "watch": "tsc --watch",
    "test": "jest --verbose false --detectOpenHandles --collectCoverageFrom=!__tests__/*.ts",
    "test:browser": "webpack-dev-server --config webpack.sandbox.config.js",
    "codecov": "npm run test && codecov",
    "doc": "typedoc --out ./docs -m \"umd\"",
    "lint": "npm run pretty-quick && tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx' 'test/**/*.ts' --fix",
    "pretty-quick": "pretty-quick",
    "precommit": "pretty-quick --staged",
    "prepare": "npm run clean && npm run build"
  },
  "files": [
    "src/*.js",
    "src/*.ts"
  ],
  "types": "src/index.d.ts",
  "keywords": [
    "aws",
    "lambda",
    "universal",
    "isomorphic",
    "workflow"
  ],
  "author": "mpppk",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.8",
    "@types/jsonpath": "^0.2.0",
    "@types/jszip": "^3.1.4",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.0.3",
    "@types/workerpool": "^2.3.0",
    "codecov": "^3.1.0",
    "husky": "^1.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.5.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "aws-sdk": "latest",
    "jsonpath": "^1.0.0",
    "lamool": "0.0.2",
    "lodash": "^4.17.11",
    "p-iteration": "^1.1.7"
  }
}
